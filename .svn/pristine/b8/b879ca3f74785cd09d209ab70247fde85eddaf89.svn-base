.req-asterisk {
   display:inline-block;
   position:relative;   
}

.req-asterisk:after {
   content: "*";
   color: red;
   font:30px 'Trebuchet MS',helvetica,arial,verdana;
   position:absolute;
   right: -15px;
   vertical-align:middle; 
}

.req-asterisk-details {
   display:inline-block;
   position:relative;   
}

.req-asterisk-details:after {
   content: "*";
   color: red;
   font:30px 'Trebuchet MS',helvetica,arial,verdana;
   position:absolute;
   left: 124px;
   vertical-align:middle; 
}

.req-asterisk-notes {
   display:inline-block;
   position:relative;   
}

.req-asterisk-notes:after {
   content: "*";
   color: red;
   font:30px 'Trebuchet MS',helvetica,arial,verdana;
   position:absolute;
   left: 57px;
   vertical-align:middle; 
}

